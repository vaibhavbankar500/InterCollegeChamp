                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- <activity android:name=".RegisterActivity" /> -->
        <activity android:name="com.example.vaibhav.intercollegechamp.LoginActivity" />
        <activity android:name="com.example.vaibhav.intercollegechamp.SignUpActivity" />
        <activity android:name="com.example.vaibhav.intercollegechamp.MainActivity" />
        <activity android:name="com.example.vaibhav.intercollegechamp.StudentSignUpActivity" />
