        <activity android:name="com.example.vaibhav.intercollegechamp.EventListActivity" />
        <activity android:name="com.example.vaibhav.intercollegechamp.EventDetailsActivity" />
        <activity android:name="com.example.vaibhav.intercollegechamp.AboutUs" />
